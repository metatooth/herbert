{
  "name": "herbert",
  "version": "0.37.0",
  "description": "automate your horticulture",
  "main": "index.js",
  "engines": {
    "node": "v18.16.0",
    "npm": "9.5.1"
  },
  "scripts": {
    "test": "NODE_ENV=unit_test WSS_URL=ws://localhost:5000 jest",
    "clean": "rm -rf dist",
    "lint": "vue-cli-service lint",
    "serve:client": "vue-cli-service serve src/client/main.ts",
    "serve:controller": "tsc-watch --noClear -b ./src/controller/tsconfig.json --onSuccess \"node --enable-source-maps dist/controller/main.js\"",
    "serve:server": "tsc-watch --noClear -b ./src/server/tsconfig.json --onSuccess \"node --enable-source-maps dist/server/main.js\"",
    "serve:socket-server": "tsc-watch --noClear -b ./src/socket-server/tsconfig.json --onSuccess \"node --enable-source-maps dist/socket-server/main.js\"",
    "serve:worker": "tsc-watch --noClear -b ./src/worker/tsconfig.json --onSuccess \"node --enable-source-maps dist/worker/main.js\"",
    "start": "node index.js",
    "copy:favicon": "cp src/server/favicon.ico dist/server/",
    "build:client": "vue-cli-service build src/client/main.ts",
    "build:controller": "tsc -b src/controller",
    "build:server": "tsc -b src/server && npm run copy:favicon",
    "build:socket-server": "tsc -b src/socket-server",
    "build:worker": "tsc -b src/worker",
    "serve": "concurrently --prefix \"[{name}]\" --names \"CLIENT,SERVER,SOCKET,WORKER\" -c \"bgBlue.bold,bgGreen.bold,bgOrange.bold,bgCyan.bold,bgRed.bold\"  \"npm run serve:client\" \"npm run serve:server\" \"npm run server:controller\" \"npm run serve:socket-server\" \"npm run serve:worker\"",
    "build": "concurrently --prefix \"[{name}]\" --names \"CLIENT,SERVER,SOCKET,WORKER\" -c \"bgBlue.bold,bgGreen.bold,bgOrange.bold,bgCyan.bold,bgRed.bold\"  \"npm run build:client\" \"npm run build:server\" \"npm run build:controller\" \"npm run build:socket-server\" \"npm run build:worker\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metatooth/herbert.git"
  },
  "license": "MIT",
  "author": "Terry Lorber",
  "dependencies": {
    "@achrinza/node-ipc": "^10.1.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@types/node": "^14.14.41",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bulma": "^0.9.2",
    "chart.js": "^2.9.4",
    "chartjs-adapter-date-fns": "^1.0.0",
    "config": "^3.3.3",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "date-fns": "^2.17.0",
    "date-fns-timezone": "^0.1.4",
    "date-fns-tz": "^1.1.3",
    "express": "^4.17.1",
    "express-promise-router": "^4.0.1",
    "meross-cloud": "^1.8.1",
    "node-switchbot": "^1.0.0",
    "onoff": "^6.0.1",
    "pg": "^8.5.1",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0",
    "tslib": "^2.1.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-cookies": "^1.7.4",
    "vue-property-decorator": "^9.1.2",
    "vue-resize-directive": "^1.2.0",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2",
    "ws": "^7.4.3",
    "wyze-node": "^2.0.3"
  },
  "devDependencies": {
    "@types/config": "0.0.38",
    "@types/cors": "^2.8.5",
    "@types/jest": "^27.0.0",
    "@types/pg": "^8.5.1",
    "@types/serve-favicon": "^2.5.2",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.2",
    "eslint": "^8.39.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "html-webpack-plugin": "^4.0.0",
    "jest": "^29.5.0",
    "node-sass": "^8.0.0",
    "prettier": "^2.8.8",
    "sass-loader": "^10.1.1",
    "ts-jest": "^29.1.0",
    "tsc-watch": "^4.4.0",
    "typescript": "^5.0.4",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.81.0"
  }
}
